{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "radekslowikdbsoreilly"
		},
		"ls_azuresql_sales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azuresql_sales'"
		},
		"ls_blob_oreillyadfetl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_blob_oreillyadfetl'"
		},
		"ls_blob_oreillyadfetl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://oreillyadfetl.dfs.core.windows.net/"
		},
		"ls_blob_oreillyadfetl_keyvault_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().url_path}"
		},
		"ls_radekslowik_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://radekslowik-keyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "radekslowikdbsoreilly_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azuresql_sales_connectionString": {
						"value": "[parameters('ls_azuresql_sales_connectionString')]"
					},
					"ls_blob_oreillyadfetl_accountKey": {
						"value": "[parameters('ls_blob_oreillyadfetl_accountKey')]"
					},
					"ls_blob_oreillyadfetl_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_oreillyadfetl_properties_typeProperties_url')]"
					},
					"ls_radekslowik_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_radekslowik_keyvault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "radekslowikdbsoreilly_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_oreillyadfetl_keyvault_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_oreillyadfetl_keyvault_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'radekslowikdbsoreilly_ArmTemplate_0')]"
			]
		},
		{
			"name": "radekslowikdbsoreilly_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'radekslowikdbsoreilly_ArmTemplate_1')]"
			]
		}
	]
}